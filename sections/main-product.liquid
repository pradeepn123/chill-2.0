<script>
  document.addEventListener("DOMContentLoaded", function() {
    const autoDeliver = document.querySelector(".rc_block__type__autodeliver")
    if (autoDeliver) {
      var mut = new MutationObserver(function(mutations, observer) {
        for (const mutation of mutations) {
          if (mutation.type === 'attributes') {
            var rcContainer = document.querySelector("#rc_container")
            if (mutation.target.classList.contains("rc_block__type--active")) {
              rcContainer.classList.add("rc_main_container")
            } else {
              rcContainer.classList.remove("rc_main_container")
            }
          }
        }
      });
      mut.observe(document.querySelector(".rc_block__type__autodeliver"), {'attributes': true});
    }
  })
  function showRecoverPasswordForm() {
    document.getElementById('recover-password').style.display = 'block';
    document.getElementById('customer').style.display='none';
  }

  function hideRecoverPasswordForm() {
    document.getElementById('recover-password').style.display = 'none';
    document.getElementById('customer').style.display = 'block';
  }
  function showRegistrationForm() {
    document.getElementById('create-customer').style.display = 'block';
    document.getElementById('customer').style.display='none';
  }

  function hideRegistrationForm() {
    document.getElementById('create-customer').style.display = 'none';
    document.getElementById('customer').style.display = 'block';
  }
  document.addEventListener("DOMContentLoaded", function(event) {
    $('#coverScreen').hide();
  })
  var claimDrawer = document.getElementById('claimDrawerContainer')
  document.getElementById('claimDrawerButton').addEventListener('click', () => {
    claimDrawer.classList.add('claim-drawer-open');
  })
  
  {% if reset_success %}showRecoverPasswordForm();{% endif %}
  if (window.location.hash == '#recover') { showRecoverPasswordForm() }
</script>

